[Unit]
Description=TiddlyWiki
After=syslog.target

[Service]
SyslogIdentifier=tiddlywiki
Restart=always
StandardOutput=syslog
ExecStart=/usr/bin/node /usr/lib/node_modules/tiddlywiki/tiddlywiki.js /home/ec2-user/wikiwiki --listen host=0.0.0.0 port=80 username=admin password=admin

[Install]
WantedBy=multi-user.target